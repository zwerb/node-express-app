{{> widgets/breadcrumb/breadcrumb}}
<div class="card">
	<div class="card-content">
		<div class="card-header justify-content-between d-flex">
			<h3 class="card-title"> {{translation.datepicker}} </h3>
		</div>
		<div class="card-data">
			<input type="text" class="datepicker" autocomplete="off" placeholder="Birthdate">
		</div>
	</div>
</div>
{{#extend 'scripts'}}
<script type="text/javascript">
	$(document).ready(function() {
		$('.datepicker').datepicker({
			autoClose: true
		});
	});

</script>
{{/extend}}
